{
	"presets": [
		"@babel/preset-typescript"
	],
	"sourceMaps": "inline",
	"retainLines": true,
	"env": {
		"development": {
			"plugins": [
				"@babel/proposal-class-properties",
				"@babel/proposal-object-rest-spread",
				"transform-es2015-modules-commonjs"
			]
		},
		"test": {
			"plugins": [
				"@babel/proposal-class-properties",
				"@babel/proposal-object-rest-spread",
				"transform-es2015-modules-commonjs"
			]
		}
	}
}